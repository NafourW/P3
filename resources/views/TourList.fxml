<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.aau.cs.ds308e18.gui.controllers.TourListController">
   <children>
      <Label text="%label_list_of_created_tours">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox prefHeight="250.0" VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane HBox.hgrow="ALWAYS">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
              </content>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ScrollPane>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#createEmptyTourButtonAction" text="%button_create_empty_tour" textAlignment="CENTER" />
                  <HBox alignment="BOTTOM_LEFT" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <GridPane alignment="BOTTOM_LEFT" maxHeight="110.0" minWidth="250.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Button layoutX="10.0" layoutY="10.0" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#editTourButtonAction" text="%button_edit_tour" textAlignment="CENTER" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM">
                                 <GridPane.margin>
                                    <Insets />
                                 </GridPane.margin>
                              </Button>
                              <Button layoutX="10.0" layoutY="10.0" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#printTourButtonAction" text="%button_print_tour" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM">
                                 <GridPane.margin>
                                    <Insets />
                                 </GridPane.margin>
                              </Button>
                              <Button layoutX="10.0" layoutY="10.0" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#deleteTourButtonAction" text="%button_delete_tour" textAlignment="CENTER" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
                                 <GridPane.margin>
                                    <Insets />
                                 </GridPane.margin>
                              </Button>
                              <Button layoutX="10.0" layoutY="10.0" minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#releaseTourButtonAction" text="%button_release_tour" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
                                 <GridPane.margin>
                                    <Insets />
                                 </GridPane.margin>
                              </Button>
                           </children>
                        </GridPane>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <ScrollPane VBox.vgrow="ALWAYS">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </ScrollPane>
      <HBox alignment="BOTTOM_LEFT" VBox.vgrow="NEVER">
         <children>
            <Button minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#orderListButtonAction" text="%button_view_order_list" textAlignment="CENTER">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <VBox alignment="BOTTOM_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Button minHeight="50.0" minWidth="120.0" mnemonicParsing="false" onAction="#backButtonAction" text="%button_back" textAlignment="CENTER" />
               </children>
            </VBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
